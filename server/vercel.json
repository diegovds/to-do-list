{
    "github": {
        "silent": true
    },
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/routes/auth.ts"
        },
        {
            "src": "(.*)",
            "dest": "src/routes/todos.ts"
        }
    ]
}